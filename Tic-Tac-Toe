#include <iostream>
char board[3][3] ={'1','2','3','4','5','6','7','8','9'}; //starting board
//all switch statements looped for less overall code thx pls
char Xboard1[3][3] = {'X','2','3','4','5','6','7','8','9'}; //boards with X
