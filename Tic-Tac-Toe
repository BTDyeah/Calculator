  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard3[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 4:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard4[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 5:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard5[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 6:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard6[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 7:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard7[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 8:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard8[i][m] << " ";
  
    }
      std::cout << std::endl;
    } break;
  case 9:
  for(int i=0;i<3;i++)  //board with x first
    { 
    for(int m=0;m<3;m++) 
    {
      
      std::cout << Xboard9[i][m] << " ";
  
    }
      std::cout << std::endl;
    }       
}


 

  
  return 0;
}

